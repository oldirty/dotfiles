set nocompatible		" first and foremost..."{{{
filetype indent plugin on      " activates indenting for files
syntax enable			" Syntax highlighting
set tabstop=4
set expandtab
set history=5000
set autoindent
set nobackup
set wildmenu
set wildmode=list:longest
set hlsearch
set autoread
set ruler
set smartcase
set showmatch
set laststatus=2
set ai
set si

colorscheme delek
